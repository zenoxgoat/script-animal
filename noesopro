local TitleTab = Window:CreateTab("Title Farm", nil) -- Title, Image


local Button = TitleTab:CreateButton({
   Name = "reset + fire farm kill",
   Callback = function()
   while true do
    local localPlayer = game:GetService("Players").LocalPlayer -- R√©cup√©rer le joueur local

    -- V√©rifier si le joueur local a un personnage avec un Humanoid
    if localPlayer.Character and localPlayer.Character:FindFirstChild("Humanoid") then
        local args = {
            [1] = "damage",
            [2] = {
                ["EnemyHumanoid"] = localPlayer.Character.Humanoid
            }
        }
        -- Envoyer l'√©v√©nement au serveur
        game:GetService("ReplicatedStorage").SkillsInRS.RemoteEvent:FireServer(unpack(args))
    end

    -- Appeler la fonction pour mettre la vie √† 0
    local function setCharacterHealthToZero()
        local player = game.Players.LocalPlayer
        local character = player.Character

        -- V√©rifie si le joueur et son personnage existent
        if character and character:FindFirstChild("Humanoid") then
            character.Humanoid.Health = 0
        end
    end

    -- Appeler la fonction pour mettre la vie √† 0
    setCharacterHealthToZero()

    -- Attente de 0,1 seconde avant la prochaine ex√©cution
    wait(6.1)
end
   end,
})

local Button = TitleTab:CreateButton({
   Name = "kill all fire",
   Callback = function()
while true do
    local localPlayer = game:GetService("Players").LocalPlayer

    local players = game:GetService("Players"):GetPlayers()

    for i, player in ipairs(players) do
        if player == localPlayer then
            table.remove(players, i)
            break
        end
    end

    local joueur_choisi
    if #players > 0 then
        joueur_choisi = players[math.random(1, #players)]
    else
        warn("Il n'y a aucun autre joueur en ligne.")
        return
    end

    local args = {
        [1] = "damage",
        [2] = {
            ["EnemyHumanoid"] = joueur_choisi.Character.Humanoid
        }
    }

    game:GetService("ReplicatedStorage").SkillsInRS.RemoteEvent:FireServer(unpack(args))

    wait(1)
end

   end,
})

